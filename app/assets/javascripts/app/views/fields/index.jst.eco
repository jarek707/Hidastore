  <div id='fieldWrap'>
		<ul class='show labels'>
			<li class='lab'>Label</li>
			<li class='lab'>Dom Id</li>
			<li class='lab'>Elm Type</li>
			<li class='lab'>Exposure</li>
		</ul>

		<% if @fields.length: %>
			<% for fld in @fields: %>
				<ul class='show item' id="field<%= fld.id %>" data-id="<%= fld.id %>"></ul>
			<% end %>
		<% end %>
  </div>
